{
	"name": "vue-inbrowser-compiler",
	"version": "4.27.0",
	"description": "compile vue single file components right in your browser",
	"module": "lib/vue-inbrowser-compiler.esm.js",
	"main": "lib/vue-inbrowser-compiler.cjs.js",
	"types": "lib/index.d.ts",
	"scripts": {
		"compile": "rollup -c && tsc --emitDeclarationOnly -p ./tsconfig.build.json",
		"compile:watch": "rollup -c --watch && tsc --emitDeclarationOnly -p ./tsconfig.build.json --watch"
	},
	"keywords": [
		"vue",
		"compile",
		"live",
		"browser",
		"buble",
		"acorn"
	],
	"dependencies": {
		"acorn": "^6.1.1",
		"acorn-jsx": "^5.0.1",
		"buble": "^0.19.7",
		"camelcase": "^5.3.1",
		"vue-inbrowser-compiler-utils": "^4.27.0",
		"walkes": "^0.2.1"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "14.0.0",
		"@rollup/plugin-node-resolve": "^8.4.0",
		"@types/buble": "^0.19.2",
		"@vue/test-utils": "^1.0.0-beta.29",
		"rollup": "^2.23.0",
		"rollup-plugin-typescript2": "^0.22.1",
		"vue": "^2.6.10"
	},
	"author": "Bart Ledoux <ledouxb@me.com>",
	"license": "MIT"
}
